; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RandomStuPicker"
config/name_localized={
"zh_CN": "随机选人"
}
config/description="A program used to pick students"
config/version="1.3.a1"
run/main_scene="res://scenes/picker.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/image="res://assets/background.png"
config/icon="res://assets/icon.png"

[autoload]

Globals="*res://globals.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://assets/": "red",
"res://locales/": "teal",
"res://scenes/": "blue",
"res://scenes/card/": "orange",
"res://scenes/settings/": "yellow",
"res://scenes/special_scene/": "green"
}

[internationalization]

locale/translations=PackedStringArray("res://locales/de_DE.de_DE.translation", "res://locales/en_US.en_US.translation", "res://locales/es_ES.es_ES.translation", "res://locales/fr_FR.fr_FR.translation", "res://locales/ja_JP.ja_JP.translation", "res://locales/zh_CN.zh_CN.translation", "res://locales/zh_TW.zh_TW.translation")
locale/test="en_US"
locale/fallback="en_US"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
